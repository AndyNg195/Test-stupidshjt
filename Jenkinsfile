pipeline {
    agent any
    stages {
        stage('Build'){
            steps{
                echo "Do some shjt"
                sh '''
                echo "Do some bigger shjt"
                '''
            }
        }
        stage('Test'){
            steps{
                echo "Test some shjt"
                sh '''
                echo "Test some bigger shjt"
                '''
            }
        }
        stage('Deliver'){
            steps{
                echo "Deliver some shjt"
                sh '''
                echo "Deliver some bigger shjt"
                '''
            }
        }
    }
}